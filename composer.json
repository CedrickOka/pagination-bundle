{
    "name" : "coka/pagination-bundle",
    "type" : "symfony-bundle",
    "description" : "Symfony Oka pagination bundle",
    "homepage" : "https://github.com/CedrickOka/pagination-bundle",
    "license" : "MIT",
    "keywords" : [
        "twig",
        "doctrine",
        "pagination"
    ],
    "authors" : [{
            "name" : "Cedrick Oka",
            "email" : "okacedrick@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Oka\\PaginationBundle\\" : "src/"
        },
        "exclude-from-classmap" : [
            "/tests/"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Oka\\PaginationBundle\\Tests\\" : "tests/"
        }
    },
    "require" : {
        "php" : ">=8.2",
        "symfony/event-dispatcher" : "^5.4 || ^6.4 || ^7.0",
        "symfony/dependency-injection" : "^5.4 || ^6.4 || ^7.0",
        "symfony/config" : "^5.4 || ^6.4 || ^7.0",
        "symfony/translation" : "^5.4 || ^6.4 || ^7.0",
        "symfony/validator" : "^5.4 || ^6.4 || ^7.0"
    },
    "require-dev" : {
        "doctrine/doctrine-bundle" : "^2.0",
        "doctrine/mongodb-odm-bundle" : "^4.2 || ^5.0",
        "doctrine/orm" : "^2.14 || ^3.0",
        "friendsofphp/php-cs-fixer" : "^3.64",
        "phpunit/phpunit" : "^10.5 || ^11.0",
        "symfony/framework-bundle" : "^5.4 || ^6.4 || ^7.0",
        "symfony/phpunit-bridge" : "^5.4 || ^6.4 || ^7.0",
        "symfony/yaml" : "^5.4 || ^6.4 || ^7.0",
        "twig/twig" : "^3.0"
    },
    "suggest" : {
        "doctrine/doctrine-bundle" : "^2.0",
        "doctrine/mongodb-odm-bundle" : "^4.2 || ^5.0",
        "twig/twig" : "^3.0"
    },
    "config" : {
        "sort-packages" : true
    },
    "scripts" : {
        "php-cs-fixer" : "php-cs-fixer fix --config=.php-cs-fixer.dist.php -v --dry-run --stop-on-violation --using-cache=no"
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "6.x-dev"
        },
        "symfony" : {
            "allow-contrib" : "true"
        }
    },
    "support" : {
        "docs" : "https://github.com/CedrickOka/pagination-bundle/blob/master/docs/index.md"
    }
}
