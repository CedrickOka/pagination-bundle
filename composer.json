{
	"name" : "coka/pagination-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Oka pagination bundle",
	"homepage" : "https://github.com/CedrickOka/pagination-bundle",
	"license" : "MIT",
	"keywords" : [
		"twig",
		"doctrine",
		"pagination"
	],
	"authors" : [{
			"name" : "Cedrick Oka",
			"email" : "okacedrick@gmail.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Oka\\PaginationBundle\\" : "src/"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\PaginationBundle\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=7.4",
		"doctrine/annotations" : "^1.2",
		"symfony/event-dispatcher" : "^4.4 || ^5.4",
		"symfony/dependency-injection" : "^4.4 || ^5.4",
		"symfony/config" : "^4.4 || ^5.4",
		"symfony/translation" : "^4.4 || ^5.4",
		"symfony/validator" : "^4.4 || ^5.4"
	},
	"require-dev" : {
		"doctrine/doctrine-bundle" : "^2.0",
		"doctrine/mongodb-odm-bundle" : "^4.2",
		"doctrine/orm" : "^2.4.8",
		"friendsofphp/php-cs-fixer" : "^3",
		"symfony/framework-bundle" : "^4.4 || ^5.4",
		"symfony/phpunit-bridge" : "^4.4 || ^5.4",
		"symfony/yaml" : "^4.4 || ^5.4",
		"twig/twig" : "^2.12|^3.0"
	},
	"suggest" : {
		"doctrine/doctrine-bundle" : "^2.0",
		"doctrine/mongodb-odm-bundle" : "^4.0",
		"twig/twig" : "^2.12|^3.0"
	},
	"config" : {
		"sort-packages" : true
	},
	"scripts" : {
		"php-cs-fixer" : "php-cs-fixer fix --config=.php-cs-fixer.dist.php -v --dry-run --stop-on-violation --using-cache=no"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "5.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/pagination-bundle/blob/master/docs/index.md"
	}
}
